<h1>Kullanıcılar</h1>
<hr>

<div class="container">
<table class="table table-hover">
    <thead>
        <tr>
        <th>#</th>
        <th>İsim</th>
        <th>Soyisim</th>
        <th>E-mail</th>
        <th>Kullanıcı Adı</th>
        <th>işlemler</th>
        </tr>
    
    <tbody>

    <%for(var i=0;i<kullanicilar.length;i++){%>
        <tr>
        <th><%=i+1%></th>
        <th><%=kullanicilar[i].name%></th>
        <th><%=kullanicilar[i].username%></th>
        <th><%=kullanicilar[i].email%></th>
        <th><%=kullanicilar[i].website%></th>
        <th><a href="/login/kullanicisil/<%=kullanicilar[i].username%>">Sil</a></th>
        </tr>
    <%}%>   

    <thead>

    </tbody>
    </thead>
</table>
</div>